<% form_for @scenario do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by, :value => current_user.username  %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
